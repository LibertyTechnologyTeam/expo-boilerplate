{
  "name": "expo-boilerplate",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "certificate": "chmod 400 .github/data/key/id_rsa && fastlane match --readonly",
    "deploy": "source /dev/stdin <<<\"$(curl -s https://raw.githubusercontent.com/tuanngocptn/semantic-versioning-action/v1/index.sh)\" && get_stage_prompt",
    "prebuild:dev": "export APP_VARIANT=dev && expo prebuild",
    "prebuild:stg": "export APP_VARIANT=stg && expo prebuild",
    "prebuild:prd": "export APP_VARIANT=prd && expo prebuild",
    "start:dev": "export APP_VARIANT=dev && expo start",
    "android:dev": "export APP_VARIANT=dev && expo start --android",
    "ios:dev": "export APP_VARIANT=dev && expo start --ios",
    "web:dev": "export APP_VARIANT=dev && expo start --web",
    "start": "expo start",
    "android": "export APP_VARIANT=dev && expo run:android",
    "ios": "export APP_VARIANT=dev && expo run:ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "lint": "expo lint",
    "tx": "node .scripts/locales.js",
    "theme": "tamagui generate-themes ./src/theme/theme-builder.ts ./src/theme/tamagui.ts",
    "prettier": "prettier --write --cache --cache-location .cache/.prettier-cache src --log-level silent",
    "cs": "node create_screen.js"
  },
  "dependencies": {
    "@gorhom/bottom-sheet": "^5.0.6",
    "@notifee/react-native": "^9.1.8",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-native-firebase/analytics": "21.6.2",
    "@react-native-firebase/app": "21.6.2",
    "@react-native-firebase/crashlytics": "21.6.2",
    "@react-native-firebase/messaging": "21.6.2",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/devtools": "^7.0.15",
    "@react-navigation/native": "^7.0.14",
    "@react-navigation/native-stack": "^7.2.0",
    "@shopify/flash-list": "^1.7.2",
    "@tamagui/cli": "^1.121.7",
    "@tamagui/config": "^1.121.7",
    "@tamagui/core": "^1.121.7",
    "@tamagui/get-token": "^1.121.7",
    "@types/i18n-js": "^3.8.9",
    "axios": "^1.7.9",
    "dayjs": "^1.11.13",
    "expo": "~52.0.23",
    "expo-build-properties": "^0.13.1",
    "expo-clipboard": "^7.0.0",
    "expo-dev-client": "~5.0.8",
    "expo-device": "^7.0.1",
    "expo-image": "^2.0.4",
    "expo-localization": "~16.0.0",
    "expo-navigation-bar": "^4.0.7",
    "expo-notifications": "^0.29.12",
    "expo-sharing": "^13.0.0",
    "expo-splash-screen": "^0.29.19",
    "expo-system-ui": "^4.0.7",
    "i18n-js": "^4.5.1",
    "immer": "^10.1.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hook-form": "^7.54.2",
    "react-icomoon": "^2.5.7",
    "react-native": "0.76.6",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-keyboard-controller": "^1.15.2",
    "react-native-mmkv": "^3.2.0",
