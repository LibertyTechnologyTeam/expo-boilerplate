    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "^5.1.0",
    "react-native-screens": "^4.5.0",
    "react-native-static-safe-area-insets": "https://github.com/mainguyen12/react-native-static-safe-area-insets.git#62d76a2acdb572dd6aa968d51c701a510c858d7e",
    "react-native-svg": "^15.11.1",
    "remeda": "^2.19.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
